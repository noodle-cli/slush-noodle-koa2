{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "private": <%= private %>,
  "main": "app.js",
  "scripts": {
    "start": "node bin/run",
    "doc": "apidoc -i ./src/ -o ./doc/"
  },
  "keywords": "",
  "precommit": "test",
  <% if (github) { %>
  "repository": {
    "type": "git",
